# Supabase Configuration (REQUIRED)
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key

# n8n Configuration (REQUIRED)
VITE_N8N_API_URL=http://your-server:5678/api/v1
VITE_N8N_API_KEY=your-n8n-api-key

# OpenAI Configuration (REQUIRED for AI Agents)
VITE_OPENAI_API_KEY=your-openai-api-key

# OAuth Configuration (OPTIONAL - for service integrations)
# Google OAuth
VITE_GOOGLE_CLIENT_ID=your-google-client-id
VITE_GOOGLE_CLIENT_SECRET=your-google-client-secret

# Microsoft OAuth
VITE_MICROSOFT_CLIENT_ID=your-microsoft-client-id
VITE_MICROSOFT_CLIENT_SECRET=your-microsoft-client-secret

# Dropbox OAuth
VITE_DROPBOX_CLIENT_ID=your-dropbox-client-id
VITE_DROPBOX_CLIENT_SECRET=your-dropbox-client-secret

# Centralized API Keys (OPTIONAL - for platform services)
# WhatsApp Business API
VITE_WHATSAPP_ACCESS_TOKEN=your-whatsapp-access-token
VITE_WHATSAPP_PHONE_NUMBER_ID=your-whatsapp-phone-number-id

# Twilio (SMS)
VITE_TWILIO_ACCOUNT_SID=your-twilio-account-sid
VITE_TWILIO_AUTH_TOKEN=your-twilio-auth-token
VITE_TWILIO_PHONE_NUMBER=your-twilio-phone-number

# SendGrid (Email)
VITE_SENDGRID_API_KEY=your-sendgrid-api-key

# Stripe (Payments)
VITE_STRIPE_SECRET_KEY=your-stripe-secret-key

# Slack
VITE_SLACK_BOT_TOKEN=your-slack-bot-token